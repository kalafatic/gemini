<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.kalafatic.gemini.bt.client.view.perspectives.ClientBasePerspective"
            fixed="false"
            icon="platform:/plugin/eu.kalafatic.gemini.core/icons/gemini/gemini.jpg"
            id="eu.kalafatic.gemini.bt.client.view.perspectives.ClientBasePerspective"
            name="%perspective.name.0">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="Client"
            name="%category.name.0"
            parentCategory="Gemini/BT">
      </category>
      <view
            category="Gemini/BT/Client"
            class="eu.kalafatic.gemini.bt.client.view.views.TorrentsTableView"
            icon="platform:/plugin/eu.kalafatic.gemini.core/icons/gemini/t.jpg"
            id="eu.kalafatic.gemini.bt.client.view.views.TorrentsTableView"
            name="%view.name.1"
            restorable="true">
      </view>
      <view
            category="Gemini/BT/Client"
            class="eu.kalafatic.gemini.bt.client.view.views.TorrentDetailView"
            id="eu.kalafatic.gemini.bt.client.view.views.TorrentDetailView"
            name="%view.name.2"
            restorable="true">
      </view>
      <view
            category="Gemini/BT/Client"
            class="eu.kalafatic.gemini.bt.client.view.views.FinishedTorrentsTableView"
            icon="platform:/plugin/eu.kalafatic.gemini.core/icons/gemini/t.jpg"
            id="eu.kalafatic.gemini.bt.client.view.views.FinishedTorrentsTableView"
            name="%view.name.3"
            restorable="true">
      </view>
      <view
            category="Gemini/BT/Client"
            class="eu.kalafatic.gemini.bt.client.view.views.TorrentDetailFilesView"
            id="eu.kalafatic.gemini.bt.client.view.views.TorrentDetailFilesView"
            name="%view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="eu.kalafatic.gemini.bt.client.controller.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            name="%page.name">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.TrackerPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.TrackerPreferencePage"
            name="%page.name.0">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.LocationsPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.LocationsPreferencePage"
            name="%page.name.1">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.NetworkPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.NetworkPreferencePage"
            name="%page.name.2">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.ThreadsPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.ThreadsPreferencePage"
            name="%page.name.3">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.MemoryPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.MemoryPreferencePage"
            name="%page.name.4">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.AlgorithmsPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.AlgorithmsPreferencePage"
            name="%page.name.5">
      </page>
      <page
            category="eu.kalafatic.gemini.bt.client.view.preferences.NetworkPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.TransportPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.TransportPreferencePage"
            name="%page.name.6">
      </page>
      <page
            category="eu.kalafatic.gemini.bt.client.view.preferences.NetworkPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.TimingPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.TimingPreferencePage"
            name="%page.name.7">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.GeminiPreferencePage/eu.kalafatic.gemini.bt.client.view.preferences.ClientPreferencePage"
            class="eu.kalafatic.gemini.bt.client.view.preferences.OptionsPreferencePage"
            id="eu.kalafatic.gemini.bt.client.view.preferences.OptionsPreferencePage"
            name="%page.name.8">
      </page>
   </extension>

</plugin>
