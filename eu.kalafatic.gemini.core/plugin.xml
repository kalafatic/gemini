            extensions="torrent,txt,xml,html,htm,jnlp"
            icon="icons/files/xml.png"
            id="eu.kalafatic.gemini.core.editors.XMLEditor"
            name="%editor.name">
      </editor>
      <editor
            class="eu.kalafatic.gemini.core.editors.EcoreEditor"
            contributorClass="org.eclipse.emf.edit.ui.action.EditingDomainActionBarContributor"
