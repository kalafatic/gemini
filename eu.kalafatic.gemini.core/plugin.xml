            class="eu.kalafatic.gemini.core.editors.MultiPageEditor"
            contributorClass="eu.kalafatic.gemini.core.editors.providers.MultiPageEditorContributor"
            default="true"
            extensions="torrent,txt,xml,html,htm,jnlp"
            icon="$nl$/icons/editor.gif"
            id="eu.kalafatic.gemini.core.editors.MultiPageEditor"
            name="%editor.name.0">
      </editor>
      <editor
