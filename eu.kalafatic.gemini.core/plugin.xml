<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="eu.kalafatic.gemini.core" name="%extension-point.name" schema="schema/eu.kalafatic.gemini.core.exsd"/>
   <extension-point id="eu.kalafatic.gemini.core.acceleratorSets" name="%extension-point.name.0" schema="schema/eu.kalafatic.gemini.core.acceleratorSets.exsd"/>
   <extension
         id="eu.kalafatic.gemini.core.application"
         name="%extension.name"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            icon="$nl$/icons/branding/tray-16.PNG"
            thread="main"
            visible="true">
         <run
               class="eu.kalafatic.gemini.core.Application">
         </run>
      </application>
   </extension>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.kalafatic.gemini.core.perspectives.CorePerspective"
            icon="$nl$/icons/toolbar/preferences.png"
            id="eu.kalafatic.gemini.core.perspectives.CorePerspective"
            name="%perspective.name.0">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="eu.kalafatic.gemini.core.perspectives.CorePerspective">
         <view
               closeable="true"
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               moveable="true"
               ratio=".65"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.ProgressView"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.TaskList"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               showTitle="true"
               standalone="true"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="Gemini"
            name="%category.name.10">
      </category>
      <category
            id="Core"
            name="%category.name.11"
            parentCategory="Gemini">
      </category>
      <category
            id="BT"
            name="%category.name.12"
            parentCategory="Gemini">
      </category>
      <view
            allowMultiple="true"
            category="Gemini/Core"
