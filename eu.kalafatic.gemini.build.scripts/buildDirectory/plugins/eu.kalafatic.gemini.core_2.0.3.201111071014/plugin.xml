   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="eu.kalafatic.gemini.core.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="eu.kalafatic.gemini.core.commands.category"
            name="%category.name">
      </category>
      <command
            categoryId="eu.kalafatic.gemini.core.commands.category"
            id="eu.kalafatic.gemini.core.commands.languageCommand"
            name="%command.name">
         <commandParameter
               id="eu.kalafatic.gemini.core.langParameter1"
               name="%commandParameter.name"
               optional="true">
         </commandParameter>
      </command>
      <command
            id="eu.kalafatic.gemini.core.openGPerspectivesCommand"
            name="%command.name.0">
      </command>
      <command
            id="eu.kalafatic.gemini.core.openGViewsCommand"
            name="%command.name.1">
      </command>
      <command
            id="eu.kalafatic.gemini.core.openWebBrowserCommand"
            name="%command.name.2">
      </command>
      <command
            id="eu.kalafatic.gemini.core.productConfigCommand"
            name="%command.name.3">
      </command>
      <command
            id="eu.kalafatic.gemini.core.restartCommand"
            name="%command.name.4">
      </command>
      <command
            id="eu.kalafatic.gemini.core.sendImprovementCommand"
            name="%command.name.5">
      </command>
      <command
            id="eu.kalafatic.gemini.core.sendProblemCommand"
            name="%command.name.6">
      </command>
      <command
            id="eu.kalafatic.gemini.core.handlers.ExitHandler"
            name="%command.name.8">
      </command>
      <command
            id="eu.kalafatic.gemini.core.handlers.KeyAssistHandler"
            name="Key Assist">
      </command>
      <command
            id="eu.kalafatic.gemini.core.handlers.UpdateHandler1"
            name="Update SW">
      </command>
      <command
            id="eu.kalafatic.gemini.core.handlers.InstallHandler"
            name="Install SW">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="eu.kalafatic.gemini.core.handlers.SelectLanguageHandler"
            commandId="eu.kalafatic.gemini.core.commands.languageCommand">
      </handler>
      <handler
            class="eu.kalafatic.gemini.core.handlers.OpenGeminiPerspectivesHandler"
            commandId="eu.kalafatic.gemini.core.openGPerspectivesCommand">
      </handler>
      <handler
            class="eu.kalafatic.gemini.core.handlers.OpenGeminiViewsHandler"
            commandId="eu.kalafatic.gemini.core.openGViewsCommand">
      </handler>
      <handler
            class="eu.kalafatic.gemini.core.handlers.OpenWebBrowserHandler"
            commandId="eu.kalafatic.gemini.core.openWebBrowserCommand">
      </handler>
      <handler
            class="eu.kalafatic.gemini.core.handlers.ProductConfigHandler"
            commandId="eu.kalafatic.gemini.core.productConfigCommand">
      </handler>
      <handler
            class="eu.kalafatic.gemini.core.handlers.RestartHandler"
