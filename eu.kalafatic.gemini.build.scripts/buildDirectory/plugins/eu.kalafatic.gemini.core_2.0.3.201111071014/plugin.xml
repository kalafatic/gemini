<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="eu.kalafatic.gemini.core" name="%extension-point.name" schema="schema/eu.kalafatic.gemini.core.exsd"/>
   <extension
         id="eu.kalafatic.gemini.core.application"
         name="%extension.name"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            icon="$nl$/icons/branding/tray-16.PNG"
            thread="main"
            visible="true">
         <run
               class="eu.kalafatic.gemini.core.Application">
         </run>
      </application>
   </extension>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.kalafatic.gemini.core.perspectives.CorePerspective"
            icon="$nl$/icons/toolbar/preferences.png"
            id="eu.kalafatic.gemini.core.conf.view.perspectives.BaseConfigurationPerspective"
            name="%perspective.name.0">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="eu.kalafatic.gemini.core.category"
            name="%category.name.0">
      </category>
      <category
            id="eu.kalafatic.gemini.core.client.category"
            name="%category.name.1"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <category
            id="eu.kalafatic.gemini.core.tm.category"
            name="%category.name.2"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <category
            id="eu.kalafatic.gemini.core.tracker.category"
            name="%category.name.3"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <category
            id="eu.kalafatic.gemini.core.media.category"
            name="Media"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <category
            id="eu.kalafatic.gemini.core.web.category"
            name="%category.name.4"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <category
            id="eu.kalafatic.gemini.core.net.category"
            name="Network"
            parentCategory="eu.kalafatic.gemini.core.category">
      </category>
      <view
            allowMultiple="true"
            category="eu.kalafatic.gemini.core.category"
            class="eu.kalafatic.gemini.core.views.FileNavigatorView"
            icon="icons/tree.gif"
            id="eu.kalafatic.gemini.core.views.FileNavigatorView"
            name="%view.name.0">
      </view>
      <view
            category="eu.kalafatic.gemini.core.category"
            class="eu.kalafatic.gemini.core.views.ConsoleView"
            icon="icons/console.png"
            id="eu.kalafatic.gemini.core.views.ConsoleView"
            name="%view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="eu.kalafatic.gemini.core.editors.MultiPageEditor"
            contributorClass="eu.kalafatic.gemini.core.editors.MultiPageEditorContributor"
            default="true"
            extensions="torrent,txt,xml,html,htm"
            icon="$nl$/icons/editor.gif"
            id="eu.kalafatic.gemini.core.editors.MultiPageEditor"
            name="%editor.name.0">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/sample.xml">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
   </extension>
   <extension 
       point="org.eclipse.ui.encodings">
      <encoding name="ISO-8859-1"/>
      <encoding name="UTF-8"/>
      <encoding name="US-ASCII"/>
   </extension>
   <extension
         id="all"
         point="org.eclipse.core.runtime.products">
      <product
            application="eu.kalafatic.gemini.core.application"
            name="Gemini">
         <property
               name="windowImages"
               value="icons/branding/tray-16.PNG,icons/branding/tray-32.PNG,icons/branding/tray-48.PNG,icons/branding/tray-64.PNG,icons/branding/tray-128.PNG">
         </property>
         <property
               name="aboutText"
               value="%aboutText">
         </property>
         <property
               name="aboutImage"
               value="icons/branding/tray-128.PNG">
         </property>
         <property
               name="startupForegroundColor"
               value="808080">
         </property>
         <property
               name="startupProgressRect"
               value="10,135,280,10">
         </property>
         <property
               name="startupMessageRect"
               value="10,115,260,20">
         </property>
         <property
               name="appName"
               value="Gemini">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="version"
               value="2.0.3">
         </property>
      </product>
   </extension>
   <extension
         id="core"
         point="org.eclipse.core.runtime.products">
      <product
            application="eu.kalafatic.gemini.core.application"
            name="%product.name.7">
         <property
               name="windowImages"
               value="icons/branding/tray-16.PNG,icons/branding/tray-32.PNG,icons/branding/tray-48.PNG,icons/branding/tray-64.PNG,icons/branding/tray-128.PNG">
