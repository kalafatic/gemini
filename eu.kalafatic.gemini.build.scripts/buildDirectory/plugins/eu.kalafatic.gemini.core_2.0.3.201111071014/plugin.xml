            commandId="eu.kalafatic.gemini.core.handlers.KeyAssistHandler"
            helpContextId="eu.kalafatic.gemini.core.handlers.KeyAssistHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="eu.kalafatic.gemini.core.commands.languageCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="menu:Tools?after=languageMarker">
         <menu
               icon="$nl$/icons/languages/language.png"
               id="eu.kalafatic.gemini.core.menus.languagesMenu"
               label="%menu.label"
               mnemonic="%menu.mnemonic">
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/czech.gif"
                  label="%command.label"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="cs">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="cs">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/english.gif"
                  label="%command.label.0"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="en">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="en">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/german.gif"
                  label="%command.label.1"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="de">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="de">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/china.gif"
                  label="%command.label.2"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="zh">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="zh">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/russia.gif"
                  label="%command.label.3"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="ru">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="ru">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
            <command
                  commandId="eu.kalafatic.gemini.core.commands.languageCommand"
                  icon="$nl$/icons/languages/flags/spain.gif"
                  label="%command.label.4"
                  style="push">
               <parameter
                     name="eu.kalafatic.gemini.core.langParameter1"
                     value="es">
               </parameter>
               <visibleWhen>
                  <not>
                     <systemTest
                           property="osgi.nl"
                           value="es">
                     </systemTest>
                  </not>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="menu:Tools?after=toolsMarker">
         <command
               commandId="eu.kalafatic.gemini.core.openWebBrowserCommand"
               icon="$nl$/icons/toolbar/web.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="eu.kalafatic.gemini.core.toolbar1">
            <command
                  commandId="eu.kalafatic.gemini.core.openWebBrowserCommand"
                  icon="$nl$/icons/toolbar/web.gif"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="menu:Tools?after=actionMarker">
         <command
               commandId="eu.kalafatic.gemini.core.restartCommand"
               icon="$nl$/icons/actions/restart.png"
               style="push">
         </command>
         <command
               commandId="eu.kalafatic.gemini.core.handlers.ExitHandler"
               icon="$nl$/icons/actions/exit.png"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:Open Perspective?after=perspMarker">
         <command
               commandId="eu.kalafatic.gemini.core.openGPerspectivesCommand"
               icon="$nl$/icons/branding/tray-16.PNG"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:Show View?after=viewMarker">
         <command
               commandId="eu.kalafatic.gemini.core.openGViewsCommand"
               icon="$nl$/icons/branding/tray-16.PNG"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:Help?after=assistMarker">
         <command
               commandId="eu.kalafatic.gemini.core.handlers.KeyAssistHandler"
               icon="icons/actions/shortcut.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:Help?after=helpMarker">
         <command
               commandId="eu.kalafatic.gemini.core.sendProblemCommand"
               icon="$nl$/icons/toolbar/bug.gif"
               style="push">
         </command>
         <command
               commandId="eu.kalafatic.gemini.core.sendImprovementCommand"
               icon="$nl$/icons/toolbar/message.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:Help?after=updateMarker">
         <command
               commandId="eu.kalafatic.gemini.core.productConfigCommand"
               icon="$nl$/icons/settings.png"
               style="push">
         </command>
         <command
               commandId="org.eclipse.equinox.p2.ui.sdk.install"
               icon="icons/actions/install.gif"
               id="org.eclipse.equinox.p2.ui.sdk.install"
               style="push">
