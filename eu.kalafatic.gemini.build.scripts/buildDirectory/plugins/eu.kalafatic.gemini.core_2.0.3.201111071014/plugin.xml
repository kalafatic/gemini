         </property>
         <property
               name="aboutImage"
               value="icons/tray_small.PNG">
         </property>
         <property
               name="startupForegroundColor"
               value="808080">
         </property>
         <property
               name="startupProgressRect"
               value="10,135,280,10">
         </property>
         <property
               name="startupMessageRect"
               value="10,115,260,20">
         </property>
         <property
               name="appName"
               value="%product.name.7">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="version"
               value="2.0.2">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            icon="$nl$/icons/gemini/gemini.jpg"
            id="eu.kalafatic.gemini.core.intro">
      </intro>
      <introProductBinding
            introId="eu.kalafatic.gemini.core.intro"
            productId="eu.kalafatic.gemini.core.all">
      </introProductBinding>
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            icon="$nl$/icons/gemini/gemini.jpg"
            id="eu.kalafatic.gemini.core.intro1">
      </intro>
      <introProductBinding
            introId="eu.kalafatic.gemini.core.intro1"
            productId="eu.kalafatic.gemini.core.core">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            content="introContent.xml"
            id="eu.kalafatic.gemini.core.introConfigId"
            introId="eu.kalafatic.gemini.core.intro">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx"
                  style="content/shared.css">
            </implementation>
         </presentation>
      </config>
      <config
            content="introContent.xml"
            id="eu.kalafatic.gemini.core.introConfigId"
            introId="eu.kalafatic.gemini.core.intro1">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx"
                  style="content/shared.css">
            </implementation>
         </presentation>
      </config>
   </extension>
