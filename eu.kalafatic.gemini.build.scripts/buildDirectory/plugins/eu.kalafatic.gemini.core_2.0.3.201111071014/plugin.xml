            id="eu.kalafatic.gemini.core.preferences.NetworkPreferencePage"
            name="%page.name.2">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.CorePreferencePage"
            class="eu.kalafatic.gemini.core.preferences.PluginsPreferencePage"
            id="eu.kalafatic.gemini.core.preferences.PluginsPreferencePage"
            name="%page.name.3">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.CorePreferencePage"
            class="eu.kalafatic.gemini.core.preferences.LocationsPreferencePage"
