            id="eu.kalafatic.gemini.core.preferences.CorePreferencePage"
            name="%page.name.0">
      </page>
      <page
            category="eu.kalafatic.gemini.core.preferences.CorePreferencePage"
            class="eu.kalafatic.gemini.core.preferences.SystemPreferencePage"
            id="eu.kalafatic.gemini.core.preferences.SystemPreferencePage"
            name="%page.name.1">
      </page>
      <page
