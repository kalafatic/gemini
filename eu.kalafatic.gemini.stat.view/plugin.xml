<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.kalafatic.gemini.stat.view.perspectives.BaseStatisticsPerspective"
            icon="platform:/plugin/eu.kalafatic.gemini.core/icons/stat.gif"
            id="eu.kalafatic.gemini.stat.view.perspectives.BaseStatisticsPerspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="Statistics"
            name="%category.name"
            parentCategory="Gemini">
      </category>
      <view
            category="Gemini/Statistics"
            class="eu.kalafatic.gemini.stat.view.views.StatisticsView"
            icon="icons/chart_bar.png"
            id="eu.kalafatic.gemini.stat.view.views.StatisticsView"
            name="%view.name"
            restorable="true">
      </view>
      <view
            category="Gemini/Statistics"
            class="eu.kalafatic.gemini.stat.view.views.StatTreeView"
            icon="platform:/plugin/eu.kalafatic.gemini.core/icons/network/swarm.png"
            id="eu.kalafatic.gemini.stat.view.views.StatTreeView"
            name="%view.name.0"
            restorable="true">
      </view>
   </extension>

</plugin>
